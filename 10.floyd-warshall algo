// User function template for JAVA

class Solution {
    public void floydWarshall(int[][] graph) {
        // Code here
        int V=graph.length;
    
        for (int k = 0; k < V; k++) {
            for (int i = 0; i < V; i++) {
                for (int j = 0; j < V; j++) {
                    if (graph[i][k] < 100000000 && graph[k][j] < 100000000) {
                        graph[i][j] = Math.min(graph[i][j], graph[i][k] + graph[k][j]);
                    }
                }
            }
        
}

    }
}